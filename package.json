{
  "name": "godeploy-api",
  "version": "0.1.0",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:smoke": "tsx src/smoke-tests/deploy.smoke.ts",
    "test:auth": "tsx src/smoke-tests/auth.smoke.ts",
    "start": "tsx src/main.ts",
    "dev": "tsx watch src/main.ts",
    "format": "npx -y prettier . --write",
    "db:new": "supabase migration new",
    "db:up": "supabase migration up",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:reset": "supabase db reset",
    "docker:build": "docker build -t godeploy-api .",
    "docker:run": "docker run -p 38444:38444 godeploy-api",
    "docker:stop": "docker stop godeploy-api",
    "docker:remove": "docker rm godeploy-api",
    "docker:start": "npm run docker:build && npm run docker:run",
    "docker:logs": "docker logs -f godeploy-api"
  },
