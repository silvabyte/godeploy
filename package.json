{
  "name": "godeploy-api",
  "version": "0.1.2",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:smoke": "bun run src/smoke-tests/deploy.smoke.ts",
    "test:auth": "bun run src/smoke-tests/auth.smoke.ts",
    "start": "bun run src/main.ts",
    "dev": "bun --watch src/main.ts",
    "format": "bunx prettier . --write",
    "db:new": "bunx supabase migration new",
    "db:up": "bunx supabase migration up",
    "db:push": "bunx supabase db push",
    "db:pull": "bunx supabase db pull",
    "db:reset": "bunx supabase db reset",
    "docker:build": "docker build -t godeploy-api .",
    "docker:run": "docker run -p 38444:38444 godeploy-api",
    "docker:stop": "docker stop godeploy-api",
    "docker:remove": "docker rm godeploy-api",
    "docker:start": "bun run docker:build && bun run docker:run",
    "docker:logs": "docker logs -f godeploy-api"
  },
  "author": "mat silva",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/lib-storage": "^3.758.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/sensible": "~6.0.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@hyperdx/node-opentelemetry": "^0.8.1",
    "@supabase/supabase-js": "^2.49.1",
    "@types/adm-zip": "^0.5.7",
    "@types/uuid": "^10.0.0",
    "adm-zip": "^0.5.16",
    "archiver": "^6.0.2",
    "await-to-js": "^3.0.0",
    "axios": "^1.8.3",
    "execa": "^9.5.2",
    "fastify": "~5.2.1",
    "fastify-plugin": "~5.0.1",
    "flat": "^6.0.1",
    "form-data": "^4.0.2",
    "mkdirp": "^3.0.1",
    "nanoid": "^5.1.5",
    "ora": "^8.2.0",
    "pino": "^9.6.0",
    "random-word": "^3.0.0",
    "rimraf": "^5.0.10",
    "slugify": "^1.6.6",
    "telemetry-sh": "^1.0.10",
    "undici": "^7.7.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "~18.16.9",
    "@types/node-fetch": "^2.6.12",
    "@types/random-word": "^2.0.2",
    "chalk": "^5.4.1",
    "husky": "^9.1.7",
    "prettier": "^2.6.2",
    "typescript": "~5.7.2",
    "@types/bun": "latest"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "private": true
}