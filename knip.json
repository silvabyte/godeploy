{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": ["apps/api/tests/**/*.test.ts", "apps/api/tests/**/*.spec.ts"],
	"project": [
		"apps/api/src/**/*.ts",
		"apps/api/tests/**/*.ts",
		"!apps/api/src/**/*.d.ts",
		"apps/dashboard/src/**/*.ts",
		"apps/marketing/src/**/*.ts"
	],
	"ignore": ["supabase/migrations/**", "docs/**"],
	"ignoreDependencies": ["husky", "@biomejs/biome"],
	"ignoreExportsUsedInFile": true,
	"ignoreBinaries": ["xrelease"]
}
