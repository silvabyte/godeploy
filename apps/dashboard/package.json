{
	"name": "@godeploy/ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"build": "tsc -b && vite build",
		"deploy": "bun run test && bun run build && godeploy deploy",
		"typecheck": "tsc --noEmit --project tsconfig.app.json",
		"fmt": "bunx biome format --write .",
		"format": "bunx biome format --write .",
		"lint": "bunx biome lint .",
		"lint:fix": "bunx biome lint --write .",
		"check": "bunx biome check .",
		"check:fix": "bunx biome check --write .",
		"generate:types": "bunx supabase gen types typescript --project-id \"gadyljeftebtastrldaq\" --schema public > src/services/database.types.ts",
		"preview": "vite preview",
		"knip": "bunx knip"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.0",
		"@heroicons/react": "^2.2.0",
		"@hyperdx/browser": "^0.21.2",
		"@matsilva/xtranslate": "^0.1.5",
		"@opentelemetry/api": "^1.9.0",
		"@supabase/supabase-js": "^2.49.1",
		"@tailwindcss/vite": "^4.0.14",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"debounce": "^2.2.0",
		"fast-memoize": "^2.5.2",
		"mobx": "^6.13.7",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "6.11.2",
		"recharts": "^2.15.1",
		"tailwindcss": "^4.0.14"
