		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react": "^4.3.4",
		"jsdom": "^26.0.0",
		"knip": "^5.63.0",
		"typescript": "~5.7.2",
